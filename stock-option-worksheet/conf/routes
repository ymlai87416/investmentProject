# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                                                           controllers.Application.index
#GET     /stockOption                                               controllers.Application.list
GET     /stockOption/:sehkcode                                      controllers.Application.listByCode(sehkcode: Int)
GET     /stockOptionHistory/openInterest/:sehkcode/:pricedate       controllers.Application.detailOpenInterest(sehkcode:Int, pricedate: String)
GET     /stockOptionHistory/settlePrice/:sehkcode/:pricedate        controllers.Application.detailSettlePrice(sehkcode:Int, pricedate: String)
GET     /stockOptionIV/:sehkcode/:pricedate                         controllers.Application.ivDetail(sehkcode: Int, pricedate: String)

# Restful interface
#GET     /rest/stockOptionHistory/:sehkcode/:pricedate

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

